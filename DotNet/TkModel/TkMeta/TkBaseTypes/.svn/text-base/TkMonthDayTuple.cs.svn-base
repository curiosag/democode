using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace TkMeta.TkBaseTypes
{
    public class TkMonthDayTuple
    {
        public UInt16 Month { get; set; }
        public UInt16 Day { get; set; }

        public TkMonthDayTuple()
        {
        }

        public TkMonthDayTuple(UInt16 month, UInt16 day)
        {
            Month = month;
            Day = day;
        }

    }
}
